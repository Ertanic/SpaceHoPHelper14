    "beforeBuildCommand": "yarn build",
    "beforeDevCommand": "yarn dev",
    "devPath": "http://localhost:1984/",
    "distDir": "./view/dist"
  },
  "tauri": {
    "allowlist": {
      "shell": {
        "open": true
      }
    },
    "bundle": {
      "active": false,
